doctype html
html
  head
    meta(charset="utf-8")
    title 방탄소년단 앨범공동구매 확인페이지
    link(rel="stylesheet" href="style2.css")
    link(rel="stylesheet" href="webfont.css")
  body
    #container
      .title 1차 컴백앨범 공구 확인 페이지
      .notice
      form
        .main
          if length == 0
              p.info 찾을 수 없습니다.
          else
            - for (var i = 0 ; i < check.length ; i ++)
              p.info 입금자명 <br/> #{check[i].name1}
              p.info 이름 <br/> #{check[i].name2}
              p.info 전화번호 <br/> #{check[i].phone}
              p.info 주소 <br/> #{check[i].address}
              br
              p.info 이메일 <br/> #{check[i].email}
              p.info 입금액 <br/> #{check[i].price}원
              p.info 셋트개수 <br/> #{check[i].count}개
              p.info 기타 <br/> #{check[i].etc}
        .link
          a(href="http://antzip.net/index.php?mid=notice&document_srl=11823" target="_blank") antzip
        .previous
          p
            input(type='button' class="previous_btn" value="이전" onclick="location.href='/';")
